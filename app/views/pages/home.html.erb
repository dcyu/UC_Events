<h1>Events That Members of University of Chicago Class of 2015 are attending!</h1>

<ul>
	<% @events.each do |e| %>
		<li><%= "#{e[:day]} #{e[:start_time]}" %></li>
		<li><%= link_to "#{e[:name]}", "http://www.facebook.com/events/#{e[:id]}/" %></li>
		<li><%= e[:location] %></li>
		<hr />
	<% end %>
</ul>
